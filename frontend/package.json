{
  "name": "web",
  "version": "1.60.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "clean:modules": "concurrently -n root,packages 'pnpm clean:modules:root' 'pnpm clean:modules:packages'",
    "clean:modules:root": "rm -rf node_modules",
    "clean:modules:packages": "pnpm -r clean:modules",
    "data-portal": "pnpm --filter data-portal",
    "dev": "pnpm data-portal dev",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2"
  },
  "pnpm": {
    "overrides": {
      "@babel/traverse": "^7.23.2",
      "braces": "^3.0.3",
      "cross-spawn": "^7.0.5",
      "dset": "^3.1.4",
      "rollup": "^3.29.5",
      "vite": "^4.5.3",
      "ws": "^7.5.10",
      "xmldom": "^0.6.0"
    }
  },
  "engines": {
    "pnpm": "8.10.5"
  }
}
